<?xml version="1.0" encoding="utf-8" ?>
<!-- Overview Map Configuration: 
     openInitial = 'true' or 'false'; 
     position = 'LowerRight', 'UpperRight', 'LowerLeft' or 'UpperLeft' -->
<WidgetConfig width="250" height="200" openInitial="false" position="LowerRight">
   <!-- If the base map's spatial reference is NOT 4326, 102100, 102113, 3857 or 3785, 
       the value of 'spatialReference' must be equal to the base map's spatial reference  -->
   <MaximumExtent xmin="-124.40" ymin="15.30" xmax="-67.40" ymax="58.50" spatialReference="4326"></MaximumExtent>

   <!-- MapLayer - must be a "Cached" ArcGIS Map Service and its spatial reference must be identical with
       that of the base map. If MapLayer is not defined, the first BaseMap layer will be used by default -->
   <!--
  <MapLayer serviceType="Cached" restURL="http://server.arcgisonline.com/ArcGIS/rest/services/World_Topo_Map/MapServer" proxyURL="" token="" />
  -->
</WidgetConfig>